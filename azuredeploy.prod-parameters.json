{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "envPrefix": {
            "value": "prod-",
            "metadata": {
                "description": "Enviroment prefix"
            }
        },
        "nsgSecurityRules": {
            "value": {
                "securityRules": [
                    {
                        "name": "Allow8172",
                        "properties": {
                            "description": "Allow port 8172",
                            "sourceAddressPrefix": "*",
                            "sourcePortRange": "*",
                            "destinationAddressPrefix": "*",
                            "destinationPortRange": "8172",
                            "protocol": "Tcp",
                            "direction": "Inbound",
                            "priority": 100,
                            "access": "Allow"
                        }
                    },
                    {
                        "name": "Allow80",
                        "properties": {
                            "description": "Allow port 80",
                            "sourceAddressPrefix": "*",
                            "sourcePortRange": "*",
                            "destinationAddressPrefix": "*",
                            "destinationPortRange": "80",
                            "protocol": "Tcp",
                            "direction": "Inbound",
                            "priority": 101,
                            "access": "Allow"
                        }
                    },
                    {
                        "name": "Allow443",
                        "properties": {
                            "description": "Allow port 443",
                            "sourceAddressPrefix": "*",
                            "sourcePortRange": "*",
                            "destinationAddressPrefix": "*",
                            "destinationPortRange": "443",
                            "protocol": "Tcp",
                            "direction": "Inbound",
                            "priority": 102,
                            "access": "Allow"
                        }
                    },
                    {
                        "name": "DenyPublicInbound",
                        "properties": {
                          "description": "Block all public incoming traffic",
                          "sourceAddressPrefix": "Internet",
                          "sourcePortRange": "*",
                          "destinationAddressPrefix": "VirtualNetwork",
                          "destinationPortRange": "*",
                          "protocol": "*",
                          "direction": "Inbound",
                          "priority": 103,
                          "access": "Deny"
                        }
                    }
                ]
            }
        },
        "vmName": {
            "value": "vm01"
        },
        "vmAdminUserName": {
            "value": "adminuser"
        },
        "vmAdminPassword": {
            "value": "A123456789a!"
        },
        "sqlServerAdminLogin": {
            "value": "sqlsuperadmin"
        },
        "sqlServerAdminPassword": {
            "value": "A123456789a!"
        },
        "publicIPDnsName": {
            "value": "blogge-prod-public"
        },
        "certPassword": {
            "value": "A123456789a"
          }
    }
}